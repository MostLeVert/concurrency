{
  "name": "Concurrency",
  "description": "Currency Conversion",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["contextMenus", "storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "page.html",
    "default_icon": "ikn.png"
  },
  "icons": {
    "16": "ikn.png",
    "48": "ikn.png",
    "128": "ikn.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
